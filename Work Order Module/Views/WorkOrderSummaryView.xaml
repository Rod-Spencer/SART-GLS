<UserControl x:Class="Segway.Modules.WorkOrder.WorkOrderSummaryView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:shared="http://schemas.actiprosoftware.com/winfx/xaml/shared"
             xmlns:wpfres="clr-namespace:Segway.WPFResources;assembly=WPFResources"
             xmlns:segway="http://service.segway.com/schemas/"
             xmlns:local="clr-namespace:Segway.Modules.WorkOrder"
             xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
             mc:Ignorable="d" 
             d:DesignHeight="650" d:DesignWidth="1200">

    <UserControl.Resources>
        <local:Image_Converter x:Key="ImageConverter" />

        <Style x:Key="HideMe" TargetType="{x:Type GridViewColumnHeader}">
            <Setter Property="Visibility" Value="Collapsed" />
        </Style>
    </UserControl.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Border Grid.Row="0" CornerRadius="3,3,0,0" Background="{DynamicResource {ComponentResourceKey ResourceId=ToolbarItemBrush1, TypeInTargetAssembly={x:Type wpfres:Brushes}}}" BorderBrush="DarkGray" BorderThickness="0,1" Margin="0">
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>
                <Rectangle Grid.ColumnSpan="2" Height="16" Fill="Snow" Opacity="0.5" VerticalAlignment="Top"/>
                <Image Grid.Column="0" Source="../Images/workordersnapshot.png" Height="25" Width="25" Margin="5">
                    <Image.Effect>
                        <DropShadowEffect Opacity="0.4" BlurRadius="4"/>
                    </Image.Effect>
                </Image>
                <TextBlock Grid.Column="1" Text="Work Order Snapshot" Foreground="Black" FontSize="16" Opacity="0.65" Margin="0,5,0,5" FontFamily="Tahoma" VerticalAlignment="Center" HorizontalAlignment="Left">
                <TextBlock.Effect>
                    <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                </TextBlock.Effect>
                </TextBlock>
            </Grid>
        </Border>
        <Grid Grid.Row="1" VerticalAlignment="Stretch">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="315"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>

            <!--
            <Popup Height="200" Width="500" AllowsTransparency="True" Grid.ColumnSpan="2" Placement="Center" IsOpen="{Binding Path=Message_IsOpen}" StaysOpen="False">
                <Border BorderBrush="Black" BorderThickness="1" CornerRadius="3" Background="{Binding Path=Message_Background_Color, Mode=TwoWay}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="100"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="50"/>
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Row="0" Grid.Column="1" Margin="5,5,5,5" Text="{Binding Path=Message_Text}" FontFamily="Tahoma" FontSize="15" />
                        <StackPanel Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" Orientation="Horizontal">
                            <Button Grid.Row="0" Grid.Column="1" Margin="25,5" Content="Yes" FontFamily="Tahoma" FontSize="15" Command="{Binding MessageYesCommand}"/>
                            <Button Grid.Row="0" Grid.Column="1" Margin="25,5" Content="No" FontFamily="Tahoma" FontSize="15"  Command="{Binding MessageNoCommand}" />
                        </StackPanel>
                    </Grid>
                </Border>
            </Popup>
            -->

            <Grid Grid.Column="0" VerticalAlignment="Stretch" Margin="4,5,0,0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="35"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Border Grid.Row="0" Grid.RowSpan="14" Grid.Column="0" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="#FFA9A9A9" CornerRadius="3" />
                    <segway:Header_Bar_Control Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" HeaderText="Summary" Header_FontSize="14.66" Header_ImageSource="{Binding Summary_Image}" />

                    <TextBlock Grid.Row="1" Grid.Column="0" Text="Work Order Number:" FontFamily="Tahoma" FontSize="14" Opacity="0.65" 
                               Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"
                               >
                        <TextBlock.Effect>
                            <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                        </TextBlock.Effect>
                        <TextBlock.ContextMenu>
                            <ContextMenu>
                                <MenuItem Header="Copy to Clipboard" Command="{Binding WO_CopyCommand}" />
                            </ContextMenu>
                        </TextBlock.ContextMenu>
                    </TextBlock>
                    <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding Path=Summary_Work_Order_Number, Mode=OneWay}" 
                               Foreground="{Binding Path=Work_Order_Number_Color, Mode=TwoWay}" FontFamily="Tahoma"
                               VerticalAlignment="Top" Margin="5,5,5,0" HorizontalAlignment="Right" FontSize="14" 
                               />

                    <TextBlock Grid.Row="2" Grid.Column="0" Text="PT Serial Number:" FontFamily="Tahoma" FontSize="14" Opacity="0.65" Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <TextBlock.ContextMenu>
                            <ContextMenu>
                                <MenuItem Header="Copy to Clipboard" Command="{Binding SN_CopyCommand}" />
                            </ContextMenu>
                        </TextBlock.ContextMenu>
                        <TextBlock.Effect>
                            <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                        </TextBlock.Effect>
                    </TextBlock>
                    <TextBlock Grid.Row="2" Grid.Column="1" Text="{Binding Path=Summary_PT_Serial_Number, Mode=OneWay}" FontFamily="Tahoma" FontSize="14" Foreground="#FF67A1DC" VerticalAlignment="Top" Margin="5,5,5,0" HorizontalAlignment="Right"/>
                    <TextBlock Grid.Row="3" Grid.Column="0" Text="PT Model:" FontFamily="Tahoma" FontSize="14" Opacity="0.65" Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">                        
                        <TextBlock.Effect>
                            <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                        </TextBlock.Effect>
                    </TextBlock>
                    <TextBlock Grid.Row="3" Grid.Column="1" Text="{Binding Path=Summary_PT_Model, Mode=OneWay}" FontFamily="Tahoma" FontSize="14" Foreground="#FF67A1DC" VerticalAlignment="Top" Margin="5,5,5,0" HorizontalAlignment="Right"/>

                    <TextBlock Grid.Row="4" Grid.Column="0" Text="Entered By:" FontFamily="Tahoma" FontSize="14" Opacity="0.65" Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <TextBlock.Effect>
                            <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                        </TextBlock.Effect>
                    </TextBlock>
                    <TextBlock Grid.Row="4" Grid.Column="1" Text="{Binding Path=Summary_User_Name, Mode=OneWay}" FontFamily="Tahoma" FontSize="14" Foreground="#FF67A1DC" VerticalAlignment="Top" Margin="5,5,5,0" HorizontalAlignment="Right"/>

                    <TextBlock Grid.Row="5" Grid.Column="0" Text="Entered Date:" FontFamily="Tahoma" FontSize="14" Opacity="0.65" Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <TextBlock.Effect>
                            <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                        </TextBlock.Effect>
                    </TextBlock>
                    <TextBlock Grid.Row="5" Grid.Column="1" Text="{Binding Path=Summary_Date_Entered, Mode=OneWay, StringFormat='{}{0:yyyy-MM-dd}'}" FontFamily="Tahoma" FontSize="14" Foreground="#FF67A1DC" VerticalAlignment="Top" Margin="5,5,5,0" HorizontalAlignment="Right"/>

                    <TextBlock Grid.Row="6" Grid.Column="0" Text="Technician:" FontFamily="Tahoma" FontSize="14" Opacity="0.65" Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <TextBlock.Effect>
                            <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                        </TextBlock.Effect>
                    </TextBlock>
                    <TextBlock Grid.Row="6" Grid.Column="1" Text="{Binding Path=Summary_Technician_Name, Mode=OneWay}" FontFamily="Tahoma" FontSize="14" Foreground="#FF67A1DC" VerticalAlignment="Top" Margin="5,5,5,0" HorizontalAlignment="Right"/>

                    <TextBlock Grid.Row="7" Grid.Column="0" Text="Start Date:" FontFamily="Tahoma" FontSize="14" Opacity="0.65" Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <TextBlock.Effect>
                            <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                        </TextBlock.Effect>
                    </TextBlock>
                    <TextBlock Grid.Row="7" Grid.Column="1" Text="{Binding Path=Summary_Start_Date, Mode=OneWay, StringFormat='{}{0:yyyy-MM-dd}'}" FontFamily="Tahoma" FontSize="14" Foreground="#FF67A1DC" VerticalAlignment="Top" Margin="5,5,5,0" HorizontalAlignment="Right"/>

                    <TextBlock Grid.Row="8" Grid.Column="0" Text="Warranty Unit:" FontFamily="Tahoma" FontSize="14" Opacity="0.65" Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <TextBlock.Effect>
                            <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                        </TextBlock.Effect>
                    </TextBlock>
                    <TextBlock Grid.Row="8" Grid.Column="1" Text="{Binding Path=Summary_IsWarranty, Mode=OneWay}" FontFamily="Tahoma" FontSize="14" Foreground="#FF67A1DC" VerticalAlignment="Top" Margin="5,5,5,0" HorizontalAlignment="Right"/>

                    <TextBlock Grid.Row="9" Grid.Column="0" Text="Work Order State:" FontFamily="Tahoma" FontSize="14" Opacity="0.65" Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <TextBlock.Effect>
                            <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                        </TextBlock.Effect>
                    </TextBlock>
                    <TextBlock Grid.Row="9" Grid.Column="1" Text="{Binding Path=Summary_Status, Mode=OneWay}" FontFamily="Tahoma" FontSize="14" Foreground="#FF67A1DC" VerticalAlignment="Top" Margin="5,5,5,0" HorizontalAlignment="Right"/>

                    <TextBlock Grid.Row="10" Grid.Column="0" Text="InfoKey Error Code:" FontFamily="Tahoma" FontSize="14" Opacity="0.65" Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <TextBlock.Effect>
                            <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                        </TextBlock.Effect>
                    </TextBlock>
                    <TextBlock Grid.Row="10" Grid.Column="1" Text="{Binding Path=InfoKey_Error_Code, Mode=OneWay}" FontFamily="Tahoma" FontSize="14" Foreground="#FF67A1DC" VerticalAlignment="Top" Margin="5,5,5,0" HorizontalAlignment="Right"/>

                    <TextBlock Grid.Row="11" Grid.Column="0" Text="Customer Name:" FontFamily="Tahoma" FontSize="14" Opacity="0.65" Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <TextBlock.Effect>
                            <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                        </TextBlock.Effect>
                    </TextBlock>
                    <TextBlock Grid.Row="11" Grid.Column="1" Text="{Binding Path=Summary_Customer, Mode=OneWay}" FontFamily="Tahoma" FontSize="14" Foreground="#FF67A1DC" VerticalAlignment="Top" Margin="5,5,5,5" HorizontalAlignment="Right"/>

                </Grid>
                <Grid Grid.Row="1" VerticalAlignment="Stretch" Margin="0,5,0,5">
                    <Border>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="35"/>
                                <RowDefinition Height="1*"/>
                            </Grid.RowDefinitions>
                            <segway:Header_Bar_Control Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,0,0,0" HeaderText="Task Summary" Header_FontSize="14.66" Header_ImageSource="{Binding TaskSummary_Image}" />
                            <Border Grid.Row="1" Margin="0" BorderThickness="1,0,1,1" BorderBrush="#FFA9A9A9" CornerRadius="3">
                                <ScrollViewer Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,0,2,2"
                                  ScrollViewer.VerticalScrollBarVisibility="Auto" VerticalAlignment="Stretch" ScrollViewer.CanContentScroll="True">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <CheckBox Grid.Row="0" Content="Starting Configuration" IsChecked="{Binding Path=IsConfigStart, Mode=OneWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" 
                                                  Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"
                                                  Visibility="{Binding Path=Summary_Start_Config_Visibility, Mode=TwoWay}"
                                                  >
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <CheckBox Grid.Row="0" Content="Start Config Override" IsChecked="True" Foreground="CadetBlue"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" 
                                                  Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"
                                                  Visibility="{Binding Path=Summary_Start_Config_Override_Visibility, Mode=TwoWay}"
                                                  >
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <CheckBox Grid.Row="1" Content="CU-A Log Extracted" IsChecked="{Binding Path=IsCUA_Extracted, Mode=OneWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <CheckBox Grid.Row="2" Content="CU-B Log Extracted" IsChecked="{Binding Path=IsCUB_Extracted, Mode=OneWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <CheckBox Grid.Row="3" Content="CU-A Code Loaded" IsChecked="{Binding Path=IsCUA_Coded, Mode=OneWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <CheckBox Grid.Row="4" Content="CU-B Code Loaded" IsChecked="{Binding Path=IsCUB_Coded, Mode=OneWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <CheckBox Grid.Row="5" Content="BSA Code Loaded" IsChecked="{Binding Path=IsBSA_Coded, Mode=TwoWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <CheckBox Grid.Row="6" Content="Normal Motor Tests" IsChecked="{Binding Path=IsNormalMotorTest, Mode=OneWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <CheckBox Grid.Row="7" Content="Rider Detect Tests" IsChecked="{Binding Path=IsRiderDetect, Mode=OneWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <CheckBox Grid.Row="8" Content="LED Tests" IsChecked="{Binding Path=IsLEDTest, Mode=OneWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <CheckBox Grid.Row="9" Content="BSA Tests" IsChecked="{Binding Path=IsBSATest, Mode=OneWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <CheckBox Grid.Row="11" Content="Other Tests" IsChecked="{Binding Path=IsOtherTest, Mode=OneWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <!--<CheckBox Grid.Row="10" Content="Final Configuration" IsChecked="{Binding Path=IsConfigFinal, Mode=OneWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>-->
                                        <CheckBox Grid.Row="10" Content="Final Configuration" IsChecked="{Binding Path=IsConfigFinal, Mode=OneWay}"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" 
                                                  Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"
                                                  Visibility="{Binding Path=Summary_Final_Config_Visibility, Mode=TwoWay}"
                                                  >
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                        <CheckBox Grid.Row="10" Content="Final Config Override" IsChecked="True" Foreground="CadetBlue"
                                                  FontFamily="Tahoma" FontSize="14" Height="25" Padding="10,0" IsEnabled="False" IsThreeState="True"
                                                  Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyle3Way}}" 
                                                  Margin="5,5,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"
                                                  Visibility="{Binding Path=Summary_Final_Config_Override_Visibility, Mode=TwoWay}"
                                                  >
                                            <CheckBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </CheckBox.Effect>
                                        </CheckBox>
                                    </Grid>
                                </ScrollViewer>
                            </Border>
                        </Grid>
                    </Border>
                </Grid>
            </Grid>
            <Grid Grid.Column="1" Margin="5,0,0,1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <ScrollViewer Grid.Row="0" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                            <!--<RowDefinition Height="Auto" />-->
                        </Grid.RowDefinitions>
                        <shared:AnimatedExpander Grid.Row="0" HorizontalAlignment="Stretch" 
                                     Width="Auto" Height="Auto"
                                     VerticalAlignment="Stretch" Margin="0,5,5,0" IsExpanded="{Binding IsObservationsExpanded}"
                                     IsAutoFocusOnExpandEnabled="True" Header="Observations"
                                     IsAutoExpandOnFocusEnabled="False"
                                     IsAutoCollapseOnBlurEnabled="False" Style="{DynamicResource {ComponentResourceKey ResourceId=ExpanderStyleOne, TypeInTargetAssembly={x:Type wpfres:Expanders}}}"
                                     >
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="1*"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <TabControl HorizontalAlignment="Stretch" Margin="-2,4,2,4" SelectedIndex="{Binding Path=SelectedTabIndex, Mode=TwoWay}"
                                            Style="{DynamicResource {ComponentResourceKey ResourceId=TabControlStyleOne, TypeInTargetAssembly={x:Type wpfres:TabControls}}}"
                                            TabStripPlacement="Top" BorderThickness="1" >
                                    <TabItem Style="{DynamicResource {ComponentResourceKey ResourceId=TabItemStyleOne, TypeInTargetAssembly={x:Type wpfres:TabControls}}}" BorderThickness="0.5,1,0.5,0.5">
                                        <TabItem.Header>
                                            <TextBlock Grid.Row="3" Grid.Column="0" Text="Customer Complaint" HorizontalAlignment="Center" FontFamily="Tahoma" FontSize="13.5" Margin="0" Opacity="1" VerticalAlignment="Center">
                                            </TextBlock>
                                        </TabItem.Header>
                                        <TextBox Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:TextBoxes}, ResourceId=TextBoxStyleOne}}"
                                             BorderThickness="1.8" Margin="3,3,3,5" VerticalScrollBarVisibility="Visible"
                                             VerticalAlignment="Stretch" HorizontalScrollBarVisibility="Visible"
                                             TabIndex="7" FontSize="14" Height="268" AcceptsReturn="True" Text="{Binding Path=Summary_Complaint}"
                                             TextWrapping="WrapWithOverflow" IsReadOnly="True" VerticalContentAlignment="Top">
                                            <TextBox.Effect>
                                                <DropShadowEffect BlurRadius="4" Opacity="0.3"/>
                                            </TextBox.Effect>
                                        </TextBox>
                                    </TabItem>
                                    <TabItem Style="{DynamicResource {ComponentResourceKey ResourceId=TabItemStyleOne, TypeInTargetAssembly={x:Type wpfres:TabControls}}}">
                                        <TabItem.Header>
                                            <TextBlock Grid.Row="3" Grid.Column="0" Text="Condition of Unit" HorizontalAlignment="Center" FontFamily="Tahoma" FontSize="13.5" Margin="0" Opacity="1" VerticalAlignment="Center">
                                            </TextBlock>
                                        </TabItem.Header>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                            </Grid.RowDefinitions>
