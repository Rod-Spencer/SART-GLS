                                            <GridViewColumn Header="Motor Right P/N" DisplayMemberBinding="{Binding Motorr_Part_Number}" />
                                            <GridViewColumn Header="Motor Right Date" DisplayMemberBinding="{Binding MotorR_Date_String}" />
                                            <GridViewColumn Header="Motor Right Installer" DisplayMemberBinding="{Binding Motorr_User}" />
                                            <GridViewColumn Header="Motor Left" DisplayMemberBinding="{Binding Motorl_Serial}" />
                                            <GridViewColumn Header="Motor Left P/N" DisplayMemberBinding="{Binding Motorl_Part_Number}" />
                                            <GridViewColumn Header="Motor Left Date" DisplayMemberBinding="{Binding Motorl_Date_String}" />
                                            <GridViewColumn Header="Motor Left Installer" DisplayMemberBinding="{Binding Motorl_User}" />
                                            <GridViewColumn Header="UIC Serial" DisplayMemberBinding="{Binding UIC_Serial}" />
                                            <GridViewColumn Header="UIC SID" DisplayMemberBinding="{Binding UIC_SID}" />
                                            <GridViewColumn Header="UIC P/N" DisplayMemberBinding="{Binding UIC_Part_Number}" />
                                            <GridViewColumn Header="UIC S/W" DisplayMemberBinding="{Binding UIC_SW_Version}" />
                                            <GridViewColumn Header="UIC Type" DisplayMemberBinding="{Binding UIC_Type}" />
                                            <GridViewColumn Header="UIC Date" DisplayMemberBinding="{Binding UIC_Date_String}" />
                                            <GridViewColumn Header="UIC Installer" DisplayMemberBinding="{Binding UIC_User}" />
                                            <GridViewColumn Header="Pivot Serial" DisplayMemberBinding="{Binding Pivot_Serial}" />
                                            <GridViewColumn Header="Pivot P/N" DisplayMemberBinding="{Binding Pivot_Part_Number}" />
                                            <GridViewColumn Header="Pivot Date" DisplayMemberBinding="{Binding Pivot_Date_String}" />
                                            <GridViewColumn Header="Pivot Installer" DisplayMemberBinding="{Binding Pivot_User}" />
                                        </GridView>
                                    </ListView.View>
                                </ListView>

                                <CheckBox HorizontalAlignment="Left" VerticalAlignment="Top" Content="Show Detailed Configurations"
                                          IsChecked="{Binding Path=IsDetailedView, Mode=TwoWay}" Opacity="0.65" Margin="5"
                                          FontFamily="Tahoma" FontSize="14"  Padding="10,0" Grid.Row="1" Visibility="{Binding Path=ConfigChoiceCheckboxVisibility}"
                                          Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyleOne}}"
                                          >
                                    <CheckBox.Effect>
                                        <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                                    </CheckBox.Effect>
                                </CheckBox>
                            </Grid>

                        </shared:AnimatedExpander>
                    </Grid>
                </ScrollViewer>
                <Grid Grid.Row="1" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <!--<ColumnDefinition Width="Auto"/>-->
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <!--<CheckBox HorizontalAlignment="Left" VerticalAlignment="Top" Content="Show Detailed Configurations"
                              IsChecked="{Binding Path=IsDetailedView, Mode=TwoWay}" Opacity="0.65"
                              FontFamily="Tahoma" FontSize="14"  Padding="10,0" Grid.Row="0" Visibility="{Binding Path=ConfigChoiceCheckboxVisibility}"
                              Style="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type wpfres:CheckBoxes}, ResourceId=CheckBoxStyleOne}}"
                              >
                        <CheckBox.Effect>
                            <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                        </CheckBox.Effect>
                    </CheckBox>-->

                    <StackPanel Orientation="Horizontal" Grid.Row="1" HorizontalAlignment="Right" >
                        <Button HorizontalAlignment="Right" Width="120" Height="30" Content="Save"
                            Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                            Margin="0,5,50,10" Command="{Binding Path=SaveCommand}" VerticalAlignment="Bottom"
                            >
                            <Button.Effect>
                                <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                            </Button.Effect>
                        </Button>
                        <Button HorizontalAlignment="Right" Width="120" Height="30" Content="Cancel"
                            Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                            Margin="0,5,5,10" Command="{Binding Path=CancelCommand}" VerticalAlignment="Bottom"
                            >
                            <Button.Effect>
                                <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                            </Button.Effect>
                        </Button>
                        <Button HorizontalAlignment="Right" Width="120" Height="30" Content="Close Work Order"
                            Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                            Margin="0,5,5,10" Command="{Binding Path=CloseCommand}" VerticalAlignment="Bottom"
                            >
                            <Button.Effect>
                                <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                            </Button.Effect>
                        </Button>

                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Grid.Row="1" HorizontalAlignment="Left" >
                        <Button HorizontalAlignment="Right" Width="150" Height="30" Content="Get Start Configuration"
                                Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                                Margin="0,5,5,10" Command="{Binding ConfigStartCommand}" VerticalAlignment="Bottom" 
                                >
                            <Button.Effect>
                                <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                            </Button.Effect>
                        </Button>

                        <Button HorizontalAlignment="Right" Width="150" Height="30" Content="Get Final Configuration"
                                Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                                Margin="0,5,5,10" Command="{Binding ConfigFinalCommand}" VerticalAlignment="Bottom" 
                                >
                            <Button.Effect>
                                <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                            </Button.Effect>
                        </Button>
                    </StackPanel>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</UserControl>
