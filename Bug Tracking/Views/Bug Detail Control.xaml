<UserControl x:Class="Segway.Service.Bugs.Bug_Detail_Control"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dtpick="clr-namespace:Segway.Modules.Control;assembly=DateTimePicker"
             xmlns:cmt="clr-namespace:Segway.Modules.Controls.Comment;assembly=CommentModule"
             xmlns:wpfres="clr-namespace:Segway.WPFResources;assembly=WPFResources"
             xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
             xmlns:local="clr-namespace:Segway.Service.Bugs"
             xmlns:wo="clr-namespace:Segway.Modules.WorkOrder"
             mc:Ignorable="d" 
             d:DesignHeight="550" d:DesignWidth="1475"
             >

    <UserControl.Resources>
        <wo:Image_Converter x:Key="ImageConverter" />
        <local:Submitter_Converter x:Key="SubmitterConverter" />
    </UserControl.Resources>


    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="10" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="10" />
            <RowDefinition Height="99" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="10" />
        </Grid.RowDefinitions>

        <Border Grid.Row="1" Grid.Column="1" BorderBrush="Black" BorderThickness="1.5" />
        <Border Grid.Row="3" Grid.Column="1" BorderBrush="Black" BorderThickness="1.5" />

        <Grid Grid.Row="1" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="100" />
                <ColumnDefinition Width="120"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="125" />
                <ColumnDefinition Width="175"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="125" />
                <ColumnDefinition Width="175"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="125" />
                <ColumnDefinition Width="175"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="100" />
                <ColumnDefinition Width="120"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="30" />
                <RowDefinition Height="30" />
                <RowDefinition Height="35" />
            </Grid.RowDefinitions>


            <TextBlock Grid.Row="0" Grid.Column="0" Text="Bug Number" VerticalAlignment="Center" FontSize="14.6" Margin="5,0"/>
            <TextBlock Grid.Row="0" Grid.Column="1" Text="{Binding Path=Issue_ID, Mode=TwoWay}" VerticalAlignment="Center" FontSize="14.6" Margin="3,0"/>

            <TextBlock Grid.Row="0" Grid.Column="3" Text="Application Name" VerticalAlignment="Center" FontSize="14.6" />
            <TextBlock Grid.Row="0" Grid.Column="4" Text="{Binding Path=Application_Name, Mode=TwoWay}" VerticalAlignment="Center" FontSize="14.6" />

            <TextBlock Grid.Row="0" Grid.Column="6" Text="Component" VerticalAlignment="Center" FontSize="14.6" />
            <ComboBox  Grid.Row="0" Grid.Column="7" VerticalAlignment="Center" FontSize="14.6" 
                       ItemsSource="{Binding Path=Components, Mode=TwoWay}" SelectedItem="{Binding Path=Selected_Component, Mode=TwoWay}"
                       />

            <TextBlock Grid.Row="0" Grid.Column="9" Text="Version Number" VerticalAlignment="Center" FontSize="14.6" />
            <TextBlock Grid.Row="0" Grid.Column="10" Text="{Binding Path=Version_Number, Mode=TwoWay}" VerticalAlignment="Center" FontSize="14.6" Margin="0,0,5,0"/>

            <TextBlock Grid.Row="0" Grid.Column="12" Text="Status" VerticalAlignment="Center" FontSize="14.6" />
            <TextBlock Grid.Row="0" Grid.Column="13" Text="{Binding Path=Status, Mode=TwoWay}" VerticalAlignment="Center" FontSize="14.6" Margin="0,0,5,0"/>

            <TextBlock Grid.Row="1" Grid.Column="6" Text="Bug Type" VerticalAlignment="Center" FontSize="14.6" />
            <ComboBox  Grid.Row="1" Grid.Column="7" ItemsSource="{Binding Path=Bug_Type_List, Mode=TwoWay}" 
                       SelectedItem="{Binding Path=Selected_Type, Mode=TwoWay}" VerticalAlignment="Center" FontSize="14.6" 
                       />


            <TextBlock Grid.Row="1" Grid.Column="0" Text="Work Order" VerticalAlignment="Center" FontSize="14.6" Margin="5,0"/>
            <TextBox   Grid.Row="1" Grid.Column="1" Text="{Binding Path=Work_Order, Mode=TwoWay}" VerticalAlignment="Center" FontSize="14.6" Margin="3,0"/>

            <TextBlock Grid.Row="1" Grid.Column="3" Text="Reproducibility" VerticalAlignment="Center" FontSize="14.6" />
            <ComboBox  Grid.Row="1" Grid.Column="4" SelectedItem="{Binding Path=Frequency, Mode=TwoWay}" 
                       ItemsSource="{Binding Path=Reproducible_List, Mode=TwoWay}" VerticalAlignment="Center" FontSize="14.6" 
                       />


            <TextBlock Grid.Row="1" Grid.Column="9" Text="Severity" VerticalAlignment="Center" FontSize="14.6" />
            <ComboBox  Grid.Row="1" Grid.Column="10" VerticalAlignment="Center" FontSize="14.6" Margin="0,0,5,0"
                       ItemsSource="{Binding Path=Severity_List, Mode=TwoWay}"  SelectedItem="{Binding Path=Selected_Severity, Mode=TwoWay}"  
                       />

            <TextBlock Grid.Row="1" Grid.Column="12" Text="Priority" VerticalAlignment="Center" FontSize="14.6" />
            <ComboBox  Grid.Row="1" Grid.Column="13" VerticalAlignment="Center" FontSize="14.6" Margin="0,0,5,0"
                       ItemsSource="{Binding Path=Priority_List, Mode=TwoWay}"  SelectedItem="{Binding Path=Selected_Priority, Mode=TwoWay}"  
                       />


            <TextBlock Grid.Row="2" Grid.Column="0" Text="Title" VerticalAlignment="Center" FontSize="14.6" Margin="5,0" />
            <TextBox Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="6" FontSize="14.6" Margin="3,0,0,0"
                     Text="{Binding Path=Summary, Mode=TwoWay}" VerticalAlignment="Center" 
                     />

            <StackPanel Orientation="Horizontal" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="11" HorizontalAlignment="Right" Margin="0,0,5,0">
                <TextBlock Text="Date/Time of Occurrance" VerticalAlignment="Center" FontSize="14.6" Margin="20,0" />
                <dtpick:DateTime_Picker FontSize="14.6" HorizontalAlignment="Right" Value="{Binding Path=Occurrance_DateTime, Mode=TwoWay}" VerticalAlignment="Center" Height="28"  />
            </StackPanel>

        </Grid>

        <Popup Grid.Row="1" Grid.RowSpan="1" Grid.Column="1" Grid.ColumnSpan="8" Width="Auto" Height="Auto"
               IsOpen="{Binding Path=PopupOpen, Mode=TwoWay}" StaysOpen="False" Placement="Center">
            <Border Width="1000" Height="Auto" Background="{Binding Path=PopupColor, Mode=TwoWay}" BorderBrush="Black" BorderThickness="2" CornerRadius="10" >
                <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                    <TextBlock Text="{Binding Path=PopupMessage, Mode=TwoWay}" FontSize="16" Margin="40,25" HorizontalAlignment="Center" />
                </ScrollViewer>
            </Border>
        </Popup>


        <Grid Grid.Row="2" Grid.Column="1" Visibility="{Binding Path=Master_Visibility, Mode=TwoWay}">
            <Border  BorderBrush="Black" BorderThickness="1.5" />

        </Grid>

        <Grid Grid.Row="3" Grid.Column="1" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition Width="725"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>

            <Border Grid.Row="0" Grid.RowSpan="2" BorderBrush="Black" BorderThickness="1.5" />

            <Grid Grid.Row="0" Grid.RowSpan="2" Grid.Column="0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="30"/>
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Border Grid.Row="1"  BorderBrush="Black" BorderThickness="1.5" />
                <TextBlock Grid.Row="0" Text="Description/Conversations" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18" FontWeight="Bold"/>

                <cmt:Comment_Control Grid.Row="1" Grid.Column="0" Name="Bug_Conversations" 
                                     dp_Comment_User="{Binding Path=UserName, Mode=OneWay}"
                                     dp_Comment_Count="{Binding Path=Comment_Count, Mode=OneWayToSource}"
                                     dp_Serial_Required="False"
                                     />

            </Grid>

            <Grid Grid.Row="0" Grid.Column="1">
                <Border BorderBrush="Black" BorderThickness="1.5" />
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="40" />
                    </Grid.RowDefinitions>

                    <Border Grid.Row="1"  BorderBrush="Black" BorderThickness="1.5" />
                    <TextBlock Grid.Row="0" Text="Pictures/Images" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18" FontWeight="Bold"/>

                    <ListView Grid.Row="1" Margin="4" BorderThickness="1" FontFamily="Tahoma" 
                              ScrollViewer.CanContentScroll="True" 
                              ScrollViewer.VerticalScrollBarVisibility="Auto"   ScrollViewer.HorizontalScrollBarVisibility="Disabled" 
                              Style="{DynamicResource {ComponentResourceKey ResourceId=ListViewStyleOne, TypeInTargetAssembly={x:Type wpfres:ListViews}}}"                                          
                              BorderBrush="{DynamicResource {ComponentResourceKey ResourceId=ControlBorderBrush, TypeInTargetAssembly={x:Type wpfres:Brushes}}}"
                              ItemsSource="{Binding Path=Picture_List, Mode=TwoWay}" 
                              SelectedItem="{Binding Path=Selected_Picture, Mode=TwoWay}"
                              >
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="MouseDoubleClick">
                                <i:InvokeCommandAction Command="{Binding PictureOpenCommand}"/>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>

                        <ListView.ItemsPanel>
                            <ItemsPanelTemplate>
                                <WrapPanel Width="{Binding (FrameworkElement.ActualWidth), RelativeSource={RelativeSource AncestorType=ScrollContentPresenter}}"
                                                       ItemWidth="{Binding (ListView.View).ItemWidth, RelativeSource={RelativeSource AncestorType=ListView}}"
                                                       MinWidth="{Binding ItemWidth, RelativeSource={RelativeSource Self}}"
                                                       ItemHeight="{Binding (ListView.View).ItemHeight, RelativeSource={RelativeSource AncestorType=ListView}}" 
                                                       />
                            </ItemsPanelTemplate>
                        </ListView.ItemsPanel>

                        <ListView.View>
                            <GridView AllowsColumnReorder="True" >
                                <GridView.ColumnHeaderContainerStyle>
                                    <Style>
                                        <Setter Property="FrameworkElement.Visibility" Value="Collapsed"/>
                                    </Style>
                                </GridView.ColumnHeaderContainerStyle>
                                <GridViewColumn Width="600">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <!--<Grid  HorizontalAlignment="Stretch">-->
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="60"/>
                                                    <ColumnDefinition />
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="60" />
                                                </Grid.RowDefinitions>
                                                <Image Grid.Column="0" Source="{Binding Path=File_Name, Converter={StaticResource ImageConverter}}" Margin="2"  HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                <Grid Grid.Column="1">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                    </Grid.RowDefinitions>
                                                    <TextBlock Grid.Row="0" Text="{Binding Path=Description}" FontSize="15" Margin="10,0,0,0" VerticalAlignment="Center" />
                                                    <Grid Grid.Row="1" Margin="10,0,0,0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="175" />
                                                            <ColumnDefinition Width="*" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Grid.Column="0" Text="{Binding Path=Creation_TS}" FontSize="15" VerticalAlignment="Center" />
                                                        <TextBlock Grid.Column="1" Text="{Binding Path=Submitter_ID, Converter={StaticResource SubmitterConverter}}" FontSize="15" VerticalAlignment="Center" Margin="10,0,0,0" />
                                                    </Grid>
                                                </Grid>
                                            </Grid>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                            </GridView>
                        </ListView.View>
                    </ListView>
                    <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,-5,0,-5">
                        <Button Width="100" Height="30" Content="Edit Desc" Margin="10,0"
                                            Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                                            Command="{Binding Path=PictureEditCommand}" VerticalAlignment="Center"
                                            />
                        <Button Width="110" Height="30" Content="Open Picture" Margin="10,0"
                                            Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                                            Command="{Binding Path=PictureOpenCommand}" VerticalAlignment="Center"
                                            />
                        <Button Width="130" Height="30" Content="Add Picture (File)" Margin="10,0" VerticalContentAlignment="Center"
                                            Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                                            Command="{Binding Path=PictureAddFileCommand}" VerticalAlignment="Center"
                                            />
                        <Button Width="150" Height="30" Content="Add Picture (Clipboard)" Margin="10,0" VerticalContentAlignment="Center"
                                            Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                                            Command="{Binding Path=PictureAddClipCommand}" VerticalAlignment="Center"
                                            />
                        <Button Width="120" Height="30" Content="Remove Picture" Margin="10,0" VerticalContentAlignment="Center"
                                            Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                                            Command="{Binding Path=PictureDelCommand}" VerticalAlignment="Center"
                                            />
                        <!--<Button.Effect>
                                    <DropShadowEffect BlurRadius="4" Opacity="0.5"/>
                                </Button.Effect>-->
                        <!--</Button>-->

                    </StackPanel>
                </Grid>

            </Grid>

            <Grid Grid.Row="1" Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="50" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>



                <!--Actions-->
                <Border Grid.Row="0" Grid.RowSpan="3" Grid.Column="0" Grid.ColumnSpan="4" BorderBrush="Black" BorderThickness=".5" Visibility="{Binding Path=Master_Visibility, Mode=TwoWay}" />
                <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="4" Height="26" VerticalAlignment="Bottom" HorizontalAlignment="Center" 
                           Text="Change Status" FontSize="18" Margin="0,5,0,0" Visibility="{Binding Path=Master_Visibility, Mode=TwoWay}"
                           />

                <Button Grid.Row="1" Grid.Column="1" Content="Accept" VerticalAlignment="Bottom" FontSize="14.6" Width="120" Height="25" 
                        Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                        Command="{Binding BugStatusAcceptCommand}" Margin="0,5" Visibility="{Binding Path=Master_Visibility, Mode=TwoWay}"
                        />

                <Button Grid.Row="1" Grid.Column="2" Content="Reject" VerticalAlignment="Bottom" FontSize="14.6" Width="120" Height="25" 
                        Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                        Command="{Binding BugStatusRejectCommand}" Margin="0,5" Visibility="{Binding Path=Master_Visibility, Mode=TwoWay}"
                        />

                <StackPanel Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="4" Orientation="Horizontal" HorizontalAlignment="Center">
                    <Button Content="Start Work" VerticalAlignment="Bottom" FontSize="14.6" Width="120" Height="25" Margin="30,5"
                        Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                        Command="{Binding BugStatusStartCommand}" Visibility="{Binding Path=Master_Visibility, Mode=TwoWay}"
                        />

                    <Button Grid.Row="2" Grid.Column="2" Content="Resolve" VerticalAlignment="Bottom" FontSize="14.6" Width="120" Height="25"
                        Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                        Command="{Binding BugStatusResolveCommand}" Margin="30,5" Visibility="{Binding Path=Master_Visibility, Mode=TwoWay}"
                        />

                    <Button Grid.Row="2" Grid.Column="3" Content="Close" VerticalAlignment="Bottom" FontSize="14.6" Width="120" Height="25"
                        Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                        Command="{Binding BugStatusCloseCommand}" Margin="30,5" Visibility="{Binding Path=Master_Visibility, Mode=TwoWay}"
                        />
                </StackPanel>
                <!--Actions-->




                <Border Grid.Row="3" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="4" BorderBrush="Black" BorderThickness=".5" 
                        Visibility="{Binding Path=Master_Visibility, Mode=TwoWay}" />

                <TextBlock Grid.Row="3" Grid.Column="0" Text="Work Started:" VerticalAlignment="Center" FontSize="14.6" Margin="20,5" HorizontalAlignment="Right" />
                <TextBlock Grid.Row="3" Grid.Column="1" FontSize="14.6" Text="{Binding Path=Work_Started, Mode=TwoWay, StringFormat=d}" VerticalAlignment="Center" Margin="0,3,0,7" />
                <!--<DatePicker Grid.Row="3" Grid.Column="1" FontSize="14.6" SelectedDate="{Binding Path=Work_Started, Mode=TwoWay}" VerticalAlignment="Center" Margin="0,3,0,7"
                            Visibility="{Binding Path=Master_Visibility, Mode=TwoWay}" />-->

                <TextBlock Grid.Row="3" Grid.Column="2" Text="Work Completed:" VerticalAlignment="Center" FontSize="14.6" Margin="20,5" HorizontalAlignment="Right" />
                <TextBlock Grid.Row="3" Grid.Column="3" FontSize="14.6" Text="{Binding Path=Work_Completed, Mode=TwoWay, StringFormat=d}" VerticalAlignment="Center" Margin="0,5"/>
                <!--<DatePicker Grid.Row="3" Grid.Column="3" FontSize="14.6" SelectedDate="{Binding Path=Work_Completed, Mode=TwoWay}" VerticalAlignment="Center" Margin="0,5"
                            Visibility="{Binding Path=Master_Visibility, Mode=TwoWay}" />-->


                <TextBlock Grid.Row="4" Grid.Column="0" Text="Version Implemented:" VerticalAlignment="Center" FontSize="14.6" Margin="20,5" HorizontalAlignment="Right" />
                <TextBlock Grid.Row="4" Grid.Column="1" Text="{Binding Path=Implemented_Version, Mode=TwoWay}" VerticalAlignment="Center" FontSize="14.6" Margin="20,5" HorizontalAlignment="Center" />

                <TextBlock Grid.Row="4" Grid.Column="2" Text="Revision:" VerticalAlignment="Center" FontSize="14.6" Margin="20,5" HorizontalAlignment="Right" />
                <TextBlock Grid.Row="4" Grid.Column="3" Text="{Binding Path=Revision, Mode=TwoWay}" VerticalAlignment="Center" FontSize="14.6" Margin="20,5" HorizontalAlignment="Center" />






                <Button Grid.Row="5" Grid.Column="0" Content="Upload Log" VerticalAlignment="Center" FontSize="14.6" 
                        Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                        Width="120" Height="25"  Command="{Binding UploadLogCommand}"
                        />

                <Button Grid.Row="5" Grid.Column="1" Content="Verify Bug Fixed" VerticalAlignment="Center" FontSize="14.6" Width="130" Height="25"
                        Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                        Command="{Binding BugStatusVerifyCommand}" Margin="0,10" 
                        />

                <Button Grid.Row="5" Grid.Column="2" Content="Reject Bug Fix" VerticalAlignment="Center" FontSize="14.6" Width="130" Height="25"
                        Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                        Command="{Binding BugStatusReOpenCommand}" Margin="0,10" 
                        />


                <Button Grid.Row="5" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center"  Content="Submit Bug" FontSize="14.6" 
                        Style="{DynamicResource {ComponentResourceKey ResourceId=ButtonStyleOne, TypeInTargetAssembly={x:Type wpfres:Buttons}}}"
                        Width="150" Height="30"  Command="{Binding SubmitCommand}"/>
            </Grid>
        </Grid>
    </Grid>
</UserControl>
